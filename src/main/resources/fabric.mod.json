{
  "schemaVersion": 1,
  "id": "projectcataclysm",
  "version": "${version}",

  "name": "Project Cataclysm",
  "description": "A weapon mod dedicated to adding interesting guns, explosives, magic, and missiles!",
  "authors": [
    "Jackson Bounds",
    "Jackson Douglas",
    "Ryan Chang",
    "Eric Li"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.bird.projectcataclysm.ProjectCataclysmMod"
    ],
    "client": [
      "net.bird.projectcataclysm.ProjectCataclysmModClient"
    ]
  },
  "mixins": [
    "projectcataclysm.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
